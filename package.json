{
  "name": "poor-man-ui-framework",
  "version": "1.0.1",
  "description": "DIY UI framework",
  "main": "dist/main.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "webpack --mode=production",
    "serve": "webpack-dev-server --open --mode=development",
    "serve:vue": "vue-cli-service serve ./vue-application/main.js --open",
    "serve:all": "concurrently \"npm run serve\" \"npm run serve:vue\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/David-Desmaisons/wtuif.git"
  },
  "keywords": [
    "ui",
    "framework",
    "javascript",
    "DIY"
  ],
  "author": "David Desmaisons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/David-Desmaisons/wtuif/issues"
  },
  "homepage": "https://github.com/David-Desmaisons/wtuif#readme",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
